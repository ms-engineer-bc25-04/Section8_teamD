datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}

model User {
  id             String   @id
  name           String
  account_number String  
}

model Project {
  id                     String   @id
  name                   String
  goal_amount            Int
  deadline               DateTime
  current_amount         Int
  project_account_number String
  status                 String
}